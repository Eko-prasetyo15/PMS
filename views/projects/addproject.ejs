<%- include("../partials/navbar") %>

<body>
    <div class="container-fluid">
        <br>
        <h2>Add Project</h2>
        <hr>
        <form method="post">
            <div class="container">
                <div class="form-group row">
                    <label for="inputName" class="col-sm-2 col-form-label" name="checkName">
                        Name</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" name="inputName" placeholder="Name">
                    </div>
                </div>
            </div>
            <div class="container">
                <div class="form-group row">
                    <label for="inputBoolean" class="col-sm-2 col-form-label" name="checkMember">
                        Member</label>
                    <div class="col-sm-10">
                        <select class="custom-select" name="inputMember">
                            <option value="">Choose Member</option>

                            <option value="1">Eko Prasetyo</option>

                            <option value="2">Romi Sunade</option>

                            <option value="3">Asep Cikungunya</option>

                        </select>
                    </div>
                </div>
            </div>
            <div class="container">
                <div class="form-group row">
                    <div class="col-sm-10">

                        <a class="btn" type="submit" style="background-color: #751a52; color: #fff;" href="/users"
                            role="button">Add</a>
                        <a class="btn btn-info" href="/users" role="button">Back</a>
                    </div>
                </div>
            </div>
            <div class="flex-col-c p-t-100">
                <span class="txt1 p-b-25" style="color: #fff;">
                    Made by "Abushanum"
                </span>
            </div>
    </div>
