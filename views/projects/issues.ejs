<%- include("../partials/sidebar") %>
<!-- Full Page Intro -->
<div class="col-9"
    style="background-image: url('stylesheets/images/bg-01.jpg'); height: 50%; background-repeat: no-repeat; background-size: cover; background-position: center center;">

    <body>
        <div class="container-fluid">
            <br>
            <h2 class="text-center">Issues <small class="text-muted">#</small></h2>
            <hr>
            <div class="card">
                <div class="card-body">
                    <div class="container">
                        <h3>Filters</h3>
                        <hr>
                        <form id="form-filter" method="GET">

                            <input type="hidden" name="page" value="1">
                            <div class="container form-group row">
                                <label for="inputIssues" class="col-sm-2 col-form-label">
                                    <input type="checkbox" id="inputIssues" name="cissues"> ID</label>
                                <div class="col-sm-10">
                                    <input type="text" class="form-control" name='issues' placeholder="Issues ID">
                                </div>
                            </div>

                            <div class="container form-group row">
                                <label for="inputSubject" class="col-sm-2 col-form-label">
                                    <input type="checkbox" id="inputSubject" name="csubject"> Subject </label>
                                <div class="col-sm-10">
                                    <input type="text" class="form-control" name='subject' placeholder="Subject Issues">
                                </div>
                            </div>

                            <div class="container form-group row">
                                <label for="inputTracker" class="col-sm-2 col-form-label">
                                    <input type="checkbox" id="inputTracker" name="ctracker">
                                    Tracker</label>
                                <div class="col-sm-10">
                                    <select class="custom-select" id name="tracker">
                                        <option value="">Choose Issues Tracker</option>
                                        <option value="bug">Bug</option>
                                        <option value="feature">Feature</option>
                                        <option value="support">Support</option>
                                    </select>
                                </div>
                            </div>

                            <div class="form-group row">
                                <div class="col-sm-10">
                                    <button type="submit" class="btn btn"
                                        style="background-color: #751a52; color: #fff;">Search</button>
                                    <a class="btn btn-primary" href="/project" role="button">Show All</a>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="shadow-sm card mt-3">
                    <div class="card-body">
                        <h3 class="h3 mb-3">Option</h3>
                        <hr />
                        <div class="form-group row">
                            <div class="col-sm-2">Columns</div>
                            <div class="col-sm-2">
                                <div class="form-check mb-2">
                                    <input class="form-check-input" type="checkbox" id="check-id" name="check-id"
                                        value="true" checked />
                                    <label class="form-check-label" for="check-id">
                                        ID
                                    </label>
                                </div>
                                <div class="form-check mb-2">
                                    <input class="form-check-input" type="checkbox" id="check-tracker"
                                        name="check-tracker" value="true" checked />
                                    <label class="form-check-label" for="check-tracker">
                                        Tracker
                                    </label>
                                </div>
                                <div class="form-check mb-2">
                                    <input class="form-check-input" type="checkbox" id="check-subject"
                                        name="check-subject" value="true" checked />
                                    <label class="form-check-label" for="check-subject">
                                        Subject
                                    </label>
                                </div>
                                <div class="form-check mb-2">
                                    <input class="form-check-input" type="checkbox" id="check-description"
                                        name="check-description" value="true" checked />
                                    <label class="form-check-label" for="check-description">
                                        Description
                                    </label>
                                </div>
                            </div>
                            <div class="col-sm-2">
                                <div class="form-check mb-2">
                                    <input class="form-check-input" type="checkbox" id="check-status"
                                        name="check-status" value="true" checked />
                                    <label class="form-check-label" for="check-status">
                                        Status
                                    </label>
                                </div>
                                <div class="form-check mb-2">
                                    <input class="form-check-input" type="checkbox" id="check-priority"
                                        name="check-priority" value="true" checked />
                                    <label class="form-check-label" for="check-priority">
                                        Priority
                                    </label>
                                </div>
                                <div class="form-check mb-2">
                                    <input class="form-check-input" type="checkbox" id="check-asignee"
                                        name="check-asignee" value="true" checked />
                                    <label class="form-check-label" for="check-asignee">
                                        Asignee
                                    </label>
                                </div>
                                <div class="form-check mb-2">
                                    <input class="form-check-input" type="checkbox" id="check-start-date"
                                        name="check-start-date" value="true" checked />
                                    <label class="form-check-label" for="check-start-date">
                                        Start date
                                    </label>
                                </div>
                            </div>
                            <div class="col-sm-2">
                                <div class="form-check mb-2">
                                    <input class="form-check-input" type="checkbox" id="check-due-date"
                                        name="check-due-date" value="true" checked />
                                    <label class="form-check-label" for="check-due-date">
                                        Due date
                                    </label>
                                </div>
                                <div class="form-check mb-2">
                                    <input class="form-check-input" type="checkbox" id="check-estimated"
                                        name="check-estimated" value="true" checked />
                                    <label class="form-check-label" for="check-estimated">
                                        Estimated
                                    </label>
                                </div>
                                <div class="form-check mb-2">
                                    <input class="form-check-input" type="checkbox" id="check-done" name="check-done"
                                        value="true" checked />
                                    <label class="form-check-label" for="check-done">
                                        Done
                                    </label>
                                </div>
                                <div class="form-check mb-2">
                                    <input class="form-check-input" type="checkbox" id="check-author"
                                        name="check-author" value="true" checked />
                                    <label class="form-check-label" for="check-author">
                                        Author
                                    </label>
                                </div>
                            </div>
                            <div class="col-sm-2">
                                <div class="form-check mb-2">
                                    <input class="form-check-input" type="checkbox" id="check-spent-time"
                                        name="check-spent-time" value="true" checked />
                                    <label class="form-check-label" for="check-spent-time">
                                        Spent time
                                    </label>
                                </div>
                                <div class="form-check mb-2">
                                    <input class="form-check-input" type="checkbox" id="check-file" name="check-file"
                                        value="true" checked />
                                    <label class="form-check-label" for="check-file">
                                        File
                                    </label>
                                </div>
                                <div class="form-check mb-2">
                                    <input class="form-check-input" type="checkbox" id="check-target-version"
                                        name="check-target-version" value="true" checked />
                                    <label class="form-check-label" for="check-target-version">
                                        Target version
                                    </label>
                                </div>
                                <div class="form-check mb-2">
                                    <input class="form-check-input" type="checkbox" id="check-created-date"
                                        name="check-created-date" value="true" checked />
                                    <label class="form-check-label" for="check-created-date">
                                        Created date
                                    </label>
                                </div>
                            </div>
                            <div class="col-sm-2">
                                <div class="form-check mb-2">
                                    <input class="form-check-input" type="checkbox" id="check-updated-date"
                                        name="check-updated-date" value="true" checked />
                                    <label class="form-check-label" for="check-updated-date">
                                        Updated date
                                    </label>
                                </div>
                                <div class="form-check mb-2">
                                    <input class="form-check-input" type="checkbox" id="check-closed-date"
                                        name="check-closed-date" value="true" checked />
                                    <label class="form-check-label" for="check-closed-date">
                                        Closed date
                                    </label>
                                </div>
                                <div class="form-check mb-2">
                                    <input class="form-check-input" type="checkbox" id="check-parent-task"
                                        name="check-parent-task" value="true" checked />
                                    <label class="form-check-label" for="check-parent-task">
                                        Parent task
                                    </label>
                                </div>
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-sm-10">
                                <button type="submit" class="btn btn"
                                    style="background-color: #751a52; color: #fff;">Apply</button>
                            </div>
                        </div>
                        <div class="row mt-3">
                            <div class="col-sm">
                                <div class="table-responsive">
                                    <table class="table">
                                        <thead>
                                            <tr class="table-activate">
                                                <th scope="col-sm">ID</th>
                                                <th scope="col-sm">Tracker</th>
                                                <th scope="col-sm">Subject</th>
                                                <th scope="col-sm">Description</th>
                                                <th scope="col-sm">Status</th>
                                                <th scope="col-sm">Priority</th>
                                                <th scope="col-sm">Asignee</th>
                                                <th scope="col-sm">Start date</th>
                                                <th scope="col-sm">Due date</th>
                                                <th scope="col-sm">Estimated</th>
                                                <th scope="col-sm">Done</th>
                                                <th scope="col-sm">Author</th>
                                                <th scope="col-sm">Spent time</th>
                                                <th scope="col-sm">File</th>
                                                <th scope="col-sm">Target version</th>
                                                <th scope="col-sm">Created date</th>
                                                <th scope="col-sm">Updated date</th>
                                                <th scope="col-sm">Closed date</th>
                                                <th scope="col-sm">Parent task</th>
                                                <th scope="col-sm">Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <th scope="row">1</th>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td>
                                                    <a href="/projects/issues/edit">
                                                        <button type="button"
                                                            class="btn btn-sm btn-success btn-block mb-2" type="submit">
                                                            Edit
                                                        </button>
                                                    </a>
                                                    <button type="button" class="btn btn-sm btn-danger btn-block"
                                                        type="submit">
                                                        Delete
                                                    </button>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <nav aria-label="Page navigation">
                            <ul class="pagination justify-content-center">
                                <li class="page-item">
                                    <a class="page-link text-dark" href="#">Previous</a>
                                </li>
                                <li class="page-item">
                                    <a class="page-link text-dark" href="#">1</a>
                                </li>
                                <li class="page-item">
                                    <a class="page-link text-dark" href="#">2</a>
                                </li>
                                <li class="page-item">
                                    <a class="page-link text-dark" href="#">3</a>
                                </li>
                                <li class="page-item">
                                    <a class="page-link text-dark" href="#">Next</a>
                                </li>
                            </ul>
                        </nav>
                        <div class="form-group row">
                            <div class="col-sm-10">
                                <a class="btn btn" style="background-color: #751a52; color: #fff;"
                                    href="/project/addproject" role="button">Add</a>
                            </div>
                        </div>
                    </div>
                    <br>

                    </form>
                </div>
                <hr>
        <hr>
        <div class="flex-col-c p-t-100">
            <span class="txt1 p-b-25" style="color: #fff;">
                Made by "Abushanum"
            </span>
        </div>
</div>
</div>
</body>
</div>
<%- include("../partials/footer") %>