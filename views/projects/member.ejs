<%- include("../partials/sidebar") %>
<!-- Full Page Intro -->
<div class="col-9"
    style="background-image: url('stylesheets/images/bg-01.jpg'); height: 100%; background-repeat: no-repeat; background-size: cover; background-position: center center;">

    <body>
        <div class="container-fluid">
            <br>
            <h2 class="text-center">Members <small class="text-muted">#</small></h2>
            <p class="h5 mb-4 text-dark">#<%= result.projectid %> <%= result.name %></p>
            <hr>
            <div class="container">
                <h3>Filters</h3>
                <br>
                <form id="form-filter" method="GET">
                    <input type="hidden" name="page" value="1">
                    <div class="form-group row">
                        <label for="inputID" class="col-sm-2 col-form-label"> <input type="checkbox" name="checkID">
                            ID</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" name="inputID" placeholder="ID">
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="inputName" class="col-sm-2 col-form-label"> <input type="checkbox" name="checkName">
                            Name</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" name="inputName" placeholder="Name">
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="inpuposition" class="col-sm-2 col-form-label"> <input type="checkbox"
                                name="checkPosition">
                            Position</label>
                        <div class="col-sm-10">
                            <select class="custom-select" id="position" name="inputPosition">
                                <option value="selected">Choose Position</option>

                                <option value="Front End Developers">Front End Developers</option>

                                <option value="Back End Developers">Back End Developers</option>

                                <option value="Full Stack Developers">Full Stack Developers</option>

                            </select>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-10">
                            <button type="submit" class="btn btn"
                                style="background-color: #751a52; color: #fff;">Search</button>
                            <a class="btn btn-primary" href="/project" role="button">Show All</a>
                        </div>
                    </div>
                </form>
            </div>
            <hr>
            <div class="container">
                <form method="POST" action="/project/option">
                    <h3>Option</h3>
                    <br>
                    <div class="form-group row">
                        <div class="col-sm-2">Columns</div>
                        <div class="col-sm-10">
                            <div class="form-check">
                                <input class="form-check-input" type="checkbox" id="checkID" name="checkId"
                                    value="true">
                                <label class="form-check-label" for="checkId">
                                    ID
                                </label>
                            </div>
                            <div class="form-check">
                                <input class="form-check-input" type="checkbox" id="checkName" name="checkname"
                                    value="true">
                                <label class="form-check-label" for="checkName">
                                    Name
                                </label>
                            </div>
                            <div class="form-check">
                                <input class="form-check-input" type="checkbox" id="checkMember" name="checkmember"
                                    value="true">
                                <label class="form-check-label" for="checkMember">
                                    Member
                                </label>
                            </div>
                        </div>
                    </div>

                    <div class="form-group row">
                        <div class="col-sm-10">
                            <button type="submit" class="btn btn"
                                style="background-color: #751a52; color: #fff;">Apply</button>
                        </div>
                    </div>
                </form>
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th scope="col">ID</th>
                            <th scope="col">Name</th>
                            <th scope="col">Position</th>
                            <th scope="col">Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% data.forEach((item) => { %>
                        <tr>
                            <th scope="row"><%= item.id %></th>
                            <td><%= item.name %></td>
                            <td><%= item.role %></td>
                            <td>
                                <a href="/project/member/<%= item.projectid %>/editmember/<%= item.id %>">
                                    <button type="button" class="btn btn-sm btn-success" type="submit">
                                        Edit
                                    </button>
                                </a>
                                <a href="/project/member/<%= item.projectid %>/delete/<%= item.id %>">
                                    <button type="button" class="btn btn-sm btn-danger" type="submit">
                                        Delete
                                    </button>
                                </a>
                            </td>
                        </tr>
                        <% }); %>
                    </tbody>
                </table>
                <nav aria-label="Page navigation">
                    <ul class="pagination justify-content-center">
                        <li class="page-item">
                            <a class="page-link text-dark" href="#">Previous</a>
                        </li>
                        <li class="page-item">
                            <a class="page-link" href="#">1</a>
                        </li>
                        <li class="page-item">
                            <a class="page-link" href="#">2</a>
                        </li>
                        <li class="page-item">
                            <a class="page-link" href="#">3</a>
                        </li>
                        <li class="page-item">
                            <a class="page-link" href="#">Next</a>
                        </li>
                    </ul>
                </nav>
                <br>
                <a class="btn" style="background-color: #751a52; color: #fff;" href="/project/member/<%= result.projectid %>/addmember"
                    role="button">Add</a>
            </div>
        </div>
    </body>
    <div class="flex-col-c p-t-100">
        <span class="txt1 p-b-25" style="color: #fff;">
            Made by "Abushanum"
        </span>
    </div>
</div>
<%- include("../partials/footer") %>