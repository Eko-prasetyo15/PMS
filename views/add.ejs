    <body>
        <div class="container-fluid">
            <br>
            <h2>Add Project</h2>
            <hr>
            <form method="post">
                <div class="container">
                    <div class="form-group row">
                        <label class="col-sm-1 col-form-label">
                            Name
                        </label>
                        <div class="row col-sm-6">
                            <input class="form-control" type="text" id="String" name="string" placeholder="Name" />
                        </div>
                    </div>
                </div>
                <div class="container">
                    <div class="form-group row">
                        <label class="col-sm-1 col-form-label">
                            Member
                        </label>
                        <div class="row col-sm-6">
                            <select class="form-control" id="Bolean" name="boolean" placeholder="--Choose the bolean--">
                                <option value="">Choose Member</option>

                                <option value="1">Eko Prasetyo</option>

                                <option value="2">Romi Sunade</option>

                                <option value="3">Asep Cikungunya</option>
                            </select>
                        </div>
                    </div>

                    <div class="form-group row">
                        <div class="col-sm-10">
                            <button type="submit" class="btn btn"
                                style="background-color: #321a75; color: #fff;">Add</button>
                            <a class="btn btn-info" href="/project" role="button">Back</a>
                        </div>
                    </div>
            </form>
            <!-- Optional JavaScript -->
