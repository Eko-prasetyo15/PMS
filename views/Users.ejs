
<%- include("./partials/navbar") %>
<!-- Full Page Intro -->
<div class="view"
    style="background-image: url('stylesheets/images/bg-01.jpg'); background-repeat: no-repeat; height : 100%; background-size: cover; background-position: center center;">


    <body>
        <div class="container-fluid">
            <br>
            <h1>Users</h1>
            <hr>

            <div class="container">
                <h3>Filters</h3>
                <br>
                <form id="form-filter" method="GET">
                    <input type="hidden" name="page" value="1">
                    <div class="form-group row">
                        <label for="inputID" class="col-sm-2 col-form-label"> <input type="checkbox" name="checkID">
                            ID</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" name="inputID" placeholder="ID">
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="inputName" class="col-sm-2 col-form-label"> <input type="checkbox" name="checkName">
                            Name</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" name="inputName" placeholder="Name">
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="inputEmail" class="col-sm-2 col-form-label"> <input type="checkbox"
                                name="checkEmail">
                            Email</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" name="inputEmail" placeholder="Name">
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="inpuposition" class="col-sm-2 col-form-label"> <input type="checkbox"
                                name="checkPosition">
                            Position</label>
                        <div class="col-sm-10">
                            <select class="custom-select" name="inputMember">
                                <option value="">Choose Position</option>

                                <option value="1">Front End Developers</option>

                                <option value="2">Back End Developers</option>

                                <option value="3">Full Stack Developers</option>

                            </select>
                        </div>
                    </div>
                    <fieldset class="form-group">
                        <div class="row">
                            <legend class="col-form-label col-sm-2 pt-0">
                                <input type="checkbox" name="checkTypeJob">Type Job
                            </legend>
                            <div class="col-sm-10">
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" id="gridRadios1" name="inputTypeJob"
                                        value="Full Time" checked="">
                                    <label class="form-check-label" for="gridRadios1">
                                        Full Time
                                    </label>
                                </div>
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" id="gridRadios2" name="inputTypeJob"
                                        value="Part Time">
                                    <label class="form-check-label" for="gridRadios2">
                                        Part Time
                                    </label>
                                </div>
                            </div>
                        </div>
                    </fieldset>

                    <div class="form-group row">
                        <div class="col-sm-10">
                            <button type="submit" class="btn btn"
                                style="background-color: #751a52; color: #fff;">Search</button>
                            <a class="btn btn-primary" href="/project" role="button">Show All</a>
                        </div>
                    </div>
                </form>
            </div>
            <hr>

            <div class="container">
                <form method="POST" action="/project/option">
                    <h3>Option</h3>
                    <br>
                    <div class="form-group row">
                        <div class="col-sm-2">Columns</div>
                        <div class="col-sm-10">
                            <div class="form-check">
                                <input class="form-check-input" type="checkbox" id="checkID" name="chkid" value="true">
                                <label class="form-check-label" for="checkID">
                                    ID
                                </label>
                            </div>
                            <div class="form-check">
                                <input class="form-check-input" type="checkbox" id="checkName" name="chkname"
                                    value="true">
                                <label class="form-check-label" for="checkName">
                                    Name
                                </label>
                            </div>
                            <div class="form-check">
                                <input class="form-check-input" type="checkbox" id="checkMember" name="chkmember"
                                    value="true">
                                <label class="form-check-label" for="checkMember">
                                    Member
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-10">
                            <button type="submit" class="btn btn"
                                style="background-color: #751a52; color: #fff;">Apply</button>
                        </div>
                    </div>
                </form>

                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th scope="col">ID</th>
                            <th scope="col" hidden="">Name</th>
                            <th scope="col">Member</th>
                            <th scope="col">Action</th>
                        </tr>
                    </thead>
                    <tbody>


                    </tbody>
                </table>

                <nav aria-label="Page navigation example">
                    <ul class="pagination justify-content-center">
                        <li class="page-item">
                            <a class="page-link" href="#" aria-label="Previous">
                                <span aria-hidden="true">&laquo;</span>
                            </a>
                        </li>
                        <li class="page-item"><a class="page-link" href="#">1</a></li>
                        <li class="page-item"><a class="page-link" href="#">2</a></li>
                        <li class="page-item">
                            <a class="page-link" href="#" aria-label="Next">
                                <span aria-hidden="true">&raquo;</span>
                            </a>
                        </li>
                    </ul>
                </nav>

                <a class="btn" style="background-color: #751a52; color: #fff;" href="/project/add" role="button">Add</a>
            </div>

        </div>
    </body>
    <div class="flex-col-c p-t-100">
        <span class="txt1 p-b-25" style="color: #fff;">
            Made by "Abushanum"
        </span>
    </div>
</div>
    <%- include("./partials/footer") %>